	<queryString language="SQL">
		<![CDATA[select "CUENTA" as tipo,co.nombConc as nombre,cu.montDeud as deuda, cu.montAbon as abonado,
CASE
    WHEN cu.estdConc='P' THEN "PAGADO"
    WHEN cu.estdConc='D' THEN "DEBE"
END as estado, cu.fechRegiModi as fechaModi
from servicio se
inner join cuota cu on se.codiServ=cu.codiServ and codiClie=   $P{codiClie} 
inner join concepto co on cu.codiConc=co.codiConc
UNION
select "OPERACION" as tipo,ts.nombTipoServ as nombre,null as deuda, null  as abonado,
CASE
    WHEN op.estdOper='A' THEN "ALTA"
    WHEN op.estdOper='P' THEN "PENDIENTE"
END as estdOper,
op.fechRegiModi as fechaModi
from operacion op
INNER join servicio se on se.codiServ=op.codiServ and se.codiClie=  $P{codiClie} 
inner join tiposervicio ts on ts.codiTipoServ=op.tipoServ;]]>
	</queryString>
